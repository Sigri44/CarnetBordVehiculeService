<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>ProjetCBVS</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<display-name>connexionCompte</display-name>
		<servlet-name>connexionCompte</servlet-name>
		<jsp-file>/login/pageAcces.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>connexionCompte</servlet-name>
		<url-pattern>/login/pageAcces</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>accesAdmin</display-name>
		<servlet-name>accesAdmin</servlet-name>
		<jsp-file>/index.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>accesAdmin</servlet-name>
		<url-pattern>/accesAdmin</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>accesUser</display-name>
		<servlet-name>accesUser</servlet-name>
		<jsp-file>/formulaire/depart.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>accesUser</servlet-name>
		<url-pattern>/accesUser</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>connexion</display-name>
		<servlet-name>connexion</servlet-name>
		<servlet-class>fr.eni.projetcbvs.servlet.PageAccesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>connexion</servlet-name>
		<url-pattern>/connexion</url-pattern>
	</servlet-mapping>
</web-app>